<!DOCTYPE html>
<html lang="hu">
<head>
    <meta charset="UTF-8">
    <title>Tanösvények</title>
    <link rel="stylesheet" href="/css/main.css">>
</head>
<body>
    <h1>Tanösvények</h1>
    <table>
        <thead>
            <tr>
                <th>Azon</th>
                <th>Név</th>
                <th>Hossz (km)</th>
                <th>Állomások</th>
                <th>Idő (óra)</th>
                <th>Idegenvezetés</th>
                <th>Település</th>
                <th>Nemzeti park</th>
            </tr>
        </thead>
        <tbody>
            <% tanosvenyek.forEach(function(ut) { %>
                <tr>
                    <td><%= ut.azon %></td>
                    <td><%= ut.nev %></td>
                    <td><%= ut.hossz %></td>
                    <td><%= ut.allomas %></td>
                    <td><%= ut.ido %></td>
                    <td><%= ut.vezetes ? "Igen" : "Nem" %></td>
                    <td><%= ut.telepulesNev %></td>
                    <td><%= ut.npNev %></td>
                </tr>
            <% }); %>
        </tbody>
    </table>
</body>
</html>
